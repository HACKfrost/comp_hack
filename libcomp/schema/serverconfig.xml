<?xml version="1.0" encoding="UTF-8"?>
<objgen>
    <object name="ServerConfig" persistent="false">
        <member type="string" name="DiffieHellmanKeyPair"
            regex="^[a-fA-F0-9]{256}|$"/>
        <member type="u16" name="Port"/>
        <member type="u8" name="DatabaseType" min="0" max="1" default="0"/>
        <member type="DatabaseConfigSQLite3*" name="SQLite3Config"/>
        <member type="DatabaseConfigCassandra*" name="CassandraConfig"/>
    </object>
</objgen>
